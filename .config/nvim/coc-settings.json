{
  "diagnostic.checkCurrentLine": true,
  "diagnostic.errorSign": "✖",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "●",
  "diagnostic.hintSign": "○",
  "codeLens.enable": true,
  "java.referencesCodeLens.enabled": true,
  "diagnostic.floatConfig.border": true,
  "diagnostic.floatConfig.rounded": true,
  "diagnostic.floatConfig.title": "Diagnostics",
  "suggest.floatConfig.border": true,
  "suggest.floatConfig.rounded": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "python",
    "json",
    "javascriptreact",
    "javascript",
    "css",
    "html",
    "java",
    "xml"
  ],
  "pydocstring.formatter": "google",
  "languageserver": {
    "python": {
      "command": "pyright",
      "trace.server": "verbose",
      "filetypes": ["py"]
    }
  }
}
